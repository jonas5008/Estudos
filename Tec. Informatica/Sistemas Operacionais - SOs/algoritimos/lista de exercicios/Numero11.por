programa {

real a, b, c, delta


  funcao inicio() {

  //entrada
  escreva("Digite o valor de A: ")
  leia(a)
  //Pergunta o valor de A

  //entrada
  escreva("Digite o valor de B: ")
  leia(b)
  //Pergunta o valor de B

  //entrada
  escreva("Digite o valor de C: ")
  leia(c)
  //Pergunta o valor de C

  //processamento
  delta = b * b - 4 * a * c
  //Cálculo de Delta

  //saida
  escreva("O valor de Delta é: ", delta)
  //Ele mostra o resultado de Delta

  }
}
