programa {

    caracter letra
    inteiro num
    

  funcao inicio() {
    
    //entrada
    escreva("Letra do setor (F, V, M, etc.): ")
    leia(letra)
    //Ele pergunta letra do setor

    //entrada
    escreva("Código do funcionário: ")
    leia(num)
    //Ele pergunta o código do funcionário


    //saida
    escreva("Funcionário do setor " , letra, ",", " código " , num,"." )
    //Ele fala a letra do funcionario e código dele
  



  }
}
