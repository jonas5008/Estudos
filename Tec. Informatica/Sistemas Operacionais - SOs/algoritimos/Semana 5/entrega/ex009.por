programa {

    cadeia nome
    cadeia telefone

  funcao inicio() {
    
    //entrada
    escreva("Nome da pessoa: ")
    leia(nome)
    //Ele mostra o nome da pessoa

    //entrada
    escreva("Número de telefone: ")
    leia(telefone)
    //Ele mostra o número de telefone

    //saida
    escreva("Contato: ", nome, " - Telefone: ", telefone,"")
    //Escreve o nome do contato e o telefone



  }
}
