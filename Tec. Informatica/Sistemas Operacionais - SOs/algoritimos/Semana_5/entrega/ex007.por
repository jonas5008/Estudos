programa {

  cadeia nome
  inteiro num


  funcao inicio() {


    //entrada
    escreva("Nome do jogo: ")
    leia(nome)
    //Ele pergunta um nome de jogo

    //entrada
    escreva("Ano que começou a jogar: ")
    leia(num)
    //Ele pergunta o ano que começou a jogar

    //saida
    escreva("Você joga ", nome, " desde ", num, "\n")
    //Escreve o nome do jogo e o ano
 
    
  }
}
